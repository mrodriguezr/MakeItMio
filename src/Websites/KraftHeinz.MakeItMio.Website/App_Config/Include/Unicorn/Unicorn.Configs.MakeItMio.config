<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<unicorn>
			<configurations>
        
				<configuration name="MakeItMio.Website" description="">
					<predicate type="Unicorn.Predicates.SerializationPresetPredicate, Unicorn" singleInstance="true">
						
						<!-- Templates -->
            <include name="Templates.Feature" database="master" path="/sitecore/templates/Feature"></include>
            <include name="Templates.Project" database="master" path="/sitecore/templates/Project"></include>

            <!--Media Library-->
            <include name="MediaLibrary" database="master" path="/sitecore/media library/MakeItMio" />

            <!--Layouts-->
            <include name="Layouts" database="master" path="/sitecore/layout/Layouts" />

            <!--Placeholder settings-->
            <include name="Placeholders" database="master" path="/sitecore/layout/Placeholder Settings/Project/MakeItMio" />

            <!--Renderings-->
            <include name="Renderings.Feature" database="master" path="/sitecore/layout/Renderings/Feature" />
            <include name="Renderings.Project" database="master" path="/sitecore/layout/Renderings/Project" />

            <!--Content-->
            <include name="Content" database="master" path="/sitecore/content/MakeItMio" />
            
					</predicate>
				</configuration>
			</configurations>
		</unicorn>
	</sitecore>
</configuration>
